<%= form_tag(new_admin_page_path, :method => :get) do %>
  <%= render :partial => "layout_picker", :locals => { :new_page => true } %>
  <%= submit_tag t('.create') %>
<% end %>

<% content_for :head do %>
  <%= javascript_include_tag 'page-layout-picker' %>
  <%= stylesheet_link_tag 'page-layout-picker' %>
<% end %>
